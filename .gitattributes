# Normalize all text files
* text=auto
# Shell scripts -> LF
*.sh text eol=lf
# mvnw (Unix) -> LF
mvnw text eol=lf
# Windows batch/cmd files -> CRLF
*.bat text eol=crlf
*.cmd text eol=crlf
# Binary
*.jar filter=lfs diff=lfs merge=lfs -text
*.png binary
*.jpg binary
*.ico binary
*.class filter=lfs diff=lfs merge=lfs -text
eclipse1/**/target/** filter=lfs diff=lfs merge=lfs -text
*.war filter=lfs diff=lfs merge=lfs -text
*.tar.gz filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
**/apache-tomcat-*/** filter=lfs diff=lfs merge=lfs -text
*.app filter=lfs diff=lfs merge=lfs -text
*.exe filter=lfs diff=lfs merge=lfs -text
*.dmg filter=lfs diff=lfs merge=lfs -text
*.tgz filter=lfs diff=lfs merge=lfs -text
